/dts-v1/;

/include/ "bcm7346.dtsi"

/ {
	compatible = "brcm,bcm97346dbsmb", "brcm,bcm7346";
	model = "Broadcom BCM97346DBSMB";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x10000000>, <0x20000000 0x30000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &uart0;
	};
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&enet0 {
	status = "okay";
};

&ehci0 {
	status = "okay";
};

&ohci0 {
	status = "okay";
};

&ehci1 {
	status = "okay";
};

&ohci1 {
	status = "okay";
};

&ehci2 {
	status = "okay";
};

&ohci2 {
	status = "okay";
};

&ehci3 {
	status = "okay";
};

&ohci3 {
	status = "okay";
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "m25p80";
		spi-max-frequency = <50000000>;
		reg = <0>;
		spi-cpha;
		spi-cpol;
		#address-cells = <1>;
		#size-cells = <1>;

		cfe@0 {
			reg = <0x0 0xa0000>;
		};

		nvram@a0000 {
			reg = <0xa0000 0x10000>;
		};

		mac@b0000 {
			reg = <0xb0000 0x10000>;
		};
	};
};
