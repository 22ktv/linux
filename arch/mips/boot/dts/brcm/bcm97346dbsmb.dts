/dts-v1/;

/include/ "bcm7346.dtsi"

/ {
	compatible = "brcm,bcm97346dbsmb", "brcm,bcm7346";
	model = "Broadcom BCM97346DBSMB";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x10000000>, <0x20000000 0x30000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &uart0;
	};
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&enet0 {
	status = "okay";
};

&ehci0 {
	status = "okay";
};

&ohci0 {
	status = "okay";
};

&ehci1 {
	status = "okay";
};

&ohci1 {
	status = "okay";
};

&ehci2 {
	status = "okay";
};

&ohci2 {
	status = "okay";
};

&ehci3 {
	status = "okay";
};

&ohci3 {
	status = "okay";
};

&nand0 {
	status = "okay";

	nandcs@2 {
		compatible = "brcm,nandcs";
		reg = <2>;
		nand-ecc-step-size = <512>;
		nand-ecc-strength = <8>;
		nand-on-flash-bbt;

		#size-cells = <2>;
		#address-cells = <2>;

		flash1.rootfs0@0 {
			reg = <0x0 0x0 0x0 0x80000000>;
		};

		flash1.rootfs1@80000000 {
			reg = <0x0 0x80000000 0x0 0x80000000>;
		};
	};
};
